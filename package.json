{
  "name": "improview-monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "dotenvx run -f apps/web/.env.local -- pnpm --filter improview-web dev",
    "dev:live": "dotenvx run -f apps/web/.env.dev -- pnpm --filter improview-web dev:live",
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test",
    "format": "pnpm -r run format",
    "typecheck": "pnpm -r run typecheck",
    "storybook": "pnpm --filter @improview/ui storybook",
    "backend:serve:local": "dotenvx run -f backend/.env.local -- sh -c 'cd backend && go run ./cmd/api'",
    "backend:smoke:local": "dotenvx run -f backend/.env.local -- ./backend/scripts/run-smoke.sh --local --debug",
    "backend:smoke:dev": "dotenvx run -f backend/.env.local -- ./backend/scripts/run-smoke.sh --env dev --debug",
    "backend:smoke:setup": "dotenvx run -f backend/.env.local -- ./backend/scripts/setup-smoke-user.sh"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.1",
    "typescript": "^5.4.2",
    "vite": "^5.2.2",
    "vitest": "^1.4.0"
  }
}
